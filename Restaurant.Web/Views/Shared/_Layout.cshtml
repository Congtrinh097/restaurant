                                                </button>
                                            </div>
                                            <!-- Collect the nav links, forms, and other content for toggling -->
                                            <div class="collapse navbar-collapse" id="respmenu">
                                                <ul class="nav navbar-nav menu">
                                                    <li>
                                                       @Html.ActionLink("Trang chủ", "Index", "Home")
